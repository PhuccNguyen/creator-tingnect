{
  "name": "tingvote-creator-lab",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:build": "docker build -t tingvote/creator-lab .",
    "docker:run": "docker run -d --name tingvote-creator-lab -p 3000:3000 --restart unless-stopped tingvote/creator-lab:latest",
    "docker:stop": "docker stop tingvote-creator-lab && docker rm tingvote-creator-lab",
    "docker:logs": "docker logs -f tingvote-creator-lab",
    "docker:dev": "docker-compose up --build",
    "docker:clean": "docker system prune -f"
  },
  "dependencies": {
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.548.0",
    "motion": "^12.23.24",
    "next": "^16.0.1",
    "nextstepjs": "^2.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "tailwindcss": "^4.1.16",
    "typescript": "^5"
  }
}
